{
  "name": "sageleaf-syntax-highlighting",
  "displayName": "Sageleaf Syntax Highlighting",
  "description": "Syntax highlighting for the sageleaf language.",
  "publisher": "sageleaf",
  "repository": {
    "type": "git",
    "url": "https://github.com/PetarPeychev/sageleaf"
  },
  "version": "1.0.2",
  "engines": {
    "vscode": "^1.64.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "sageleaf",
        "aliases": [
          "sageleaf",
          "sl"
        ],
        "extensions": [
          ".sl"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "sageleaf",
        "scopeName": "source.sageleaf",
        "path": "./syntaxes/sageleaf.tmLanguage.json"
      }
    ]
  }
}
